                "verify_count": {
                    "type": "integer"
                },
                "reject_count": {
                    "type": "integer"
                },
                "fixed_at": {
                    "type": "string",
                    "format": "date-time"
                },
                "fixed_by": {
                    "type": "string"
                },
                "fix_photo_url": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string",
                    "format": "date-time"
                },
                "verified_at": {
                    "type": "string",
                    "format": "date-time"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Issue"
        }
